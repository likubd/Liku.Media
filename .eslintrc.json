{
  "parser": "@typescript-eslint/parser",

  "extends": [
    "prettier",
    "plugin:prettier/recommended",
    "react-app"
  ],

  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  },

  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  }
}